class Program {
    static void Main() {
        string[] names = {"Maria", "José", "Felipe", "Ester"};

        Console.WriteLine("Laço for");
        for(int i = 0 ; i < names.Length; i++ ){
            Console.WriteLine(names[i]);
        }


        Console.WriteLine("Laço com foreach");
        foreach (string name in names) {
            Console.WriteLine(name);
        }

        Console.WriteLine("Laço While e Do");
        int u = 0;
        do{
            
            Console.WriteLine(names[u]);
            u++;
            
        }while(u < names.Length);
    }
}
